[MeterCity]
Meter=String
MeasureName=CityNameChild
X=20
Y=15
FontSize=10
StringAlign=LeftTop
FontFace=Century Gothic
FontColor=255,255,255,255
SolidColor=0,0,0,1
StringAlign=Right
AntiAlias=1
Text=%1

[MeterCurrentIcon]
Meter=Image
X=275
Y=35
W=150
ImagePath=#@#WeatherIcons
ImageAlpha=100
MeasureName=CurrentIconChild
ColorMatrix1=-1;0;0;0;0
ColorMatrix2=0;-1;0;0;0
ColorMatrix3=0;0;-1;0;0
ColorMatrix5=1;1;1;0;1

[MeterCurrentCondition]
Meter=String
MeasureName=CurrentConditionsChild
StringAlign=CenterCenter
FontFace=Century Gothic
FontColor=255,255,255,255
AntiAlias=1
Text=Condition: %1
X=350
Y=180

[MeterCurrentTemp]
Meter=String
MeasureName=CurrentTempChild
MeasureName2=UnitsTempChild
X=20
Y=60
FontSize=50
StringAlign=LeftTop
FontFace=Century Gothic
FontColor=255,255,255,255
AntiAlias=1
Text=%1°%2

[MeterTodayMinMax]
Meter=String
MeasureName=TodayLowChild
MeasureName2=TodayHighChild
X=20
Y=180
StringAlign=LeftCenter
FontSize=11
FontFace=Century Gothic
FontColor=255,255,255,255
SolidColor=0,0,0,1
StringAlign=Right
AntiAlias=1
Text=Minimum: %1°C | Maximum: %2°C

[MeterSeperatorLine]
Meter=Shape
Shape=Rectangle 17,205,440,2 | Fill Color 255,255,255,100 | StrokeWidth 0

[MeterForecastText]
