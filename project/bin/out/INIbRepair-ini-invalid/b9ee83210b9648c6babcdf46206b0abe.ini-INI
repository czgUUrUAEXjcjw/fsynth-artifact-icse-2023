[Rainmeter]
Update=1000
AccurateText=1

[Metadata]
Name=Dash
Author= Mazeby
Information= A Rainmeter skin collection for people with too many screens and a need for some clean system monitoring tools and other various widgets.
Version= 1.0
License= CC BY-NC 

[Background]
;This is only used to get the same border width around the entire widget, it does not have any other visual purpose except this.
Meter=Shape
Shape=Rectangle 0,0,1040,340 | Fill Color 0,0,0,0 | StrokeWidth 0

;------------------------
;--------PLUGINS---------
;------------------------

[FrostedGlass]
Measure=Plugin
Plugin=FrostedGlass
Type=Acrylic
Border=All

[CoreTempSpeed]
Measure=Plugin
Plugin=CoreTemp
CoreTempType=CpuSpeed

;------------------------
;-------MEASURES---------
;------------------------

[measureCPUavg]
Measure=CPU

[measureCPU1]
Measure=CPU
Processor=1

[measureCPU2]
Measure=CPU
Processor=2

[measureCPU3]
Measure=CPU
Processor=3

[measureCPU4]
Measure=CPU
Processor=4

[measureCPU5]
Measure=CPU
Processor=5

[measureCPU6]
Measure=CPU
Processor=6

[measureCPUname]
Measure=Registry
RegHKey=HKEY_LOCAL_MACHINE
RegKey=HARDWARE\DESCRIPTION\System\CentralProcessor\0
RegValue=ProcessorNameString

[measureCPUspeed]
Measure=Calc
Formula=(CoreTempSpeed / 1000)
DynamicVariables=1

;------------------------------------------------------------------------------------------------------------------
;------------------------------------------------------------------------------------------------------------------CPU Monitoring
;------------------------------------------------------------------------------------------------------------------

[CPUtitle]
Meter=String
MeasureName=measureCPUName
FontFace=Century Gothic
Text=CPU: %1
FontColor=255,255,255,100
FontSize=15
Antialias=1
X=20
Y=20

[CPUspeed]
Meter=String
MeasureName=measureCPUspeed
NumOfDecimals=2
FontFace=Century Gothic
Text=Current Speed: %1 GHz
FontColor=255,255,255,100
FontSize=15
Antialias=1
X=600
Y=20
